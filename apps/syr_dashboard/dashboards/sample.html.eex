<div class="gridster">
  <ul>
    <li data-row="1"
        data-col="1"
        data-sizex="1"
        data-sizey="1">
      <div class="widget-synergy"
           data-widget="Meter"
           data-source="water_flow_lpm"
           data-title="Water Flow[l/10sec]"
           data-min="0"
           data-max="20"
           data-step="0.1"></div>
    </li>

    <li data-row="1"
        data-col="2"
        data-sizex="1"
