           data-source="water_pressure_bar"
           data-title="Water Pressure[bar]"
           data-min="0"
           data-max="6"
           data-step="0.1"></div>
    </li>

    <li data-row="2"
        data-col="2"
        data-sizex="1"
        data-sizey="1">
      <div class="widget-synergy"
           data-widget="Meter"
           data-source="remaining_salt"
           data-title="Remaining Salt[weeks]"
           data-min="0"
           data-max="24"
           data-step="1"></div>
    </li>
    <li data-row="3"
        data-col="1"
        data-sizex="1"
        data-sizey="1">
      <div class="widget-synergy"
           data-widget="Text"
           data-source="device_date_time"
           data-title="Date Time"
           data-text="Empty"
           </div>
    </li>

